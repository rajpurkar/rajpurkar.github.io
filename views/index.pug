            //     | Tweets by pranavrajpurkar
            //   script(async src="https://platform.twitter.com/widgets.js" charset="utf-8")
        .col-md-6
            h3 Recent Talk
            .row
              .col-sm-12
                iframe(width="100%", height='300px', src="//www.youtube.com/embed/PazX2Nzfo8w?ecver=1",frameborder="0",allow="autoplay; encrypted-media",allowfullscreen)
            hr
            h3 Research Videos
            .row
              .col-sm-12
                iframe(width="100%", height='300px', src="//www.youtube.com/embed/VJRCj-4E2iU?ecver=1",frameborder="0",allow="autoplay; encrypted-media",allowfullscreen)
                iframe(width="100%", height='300px', src="//www.youtube.com/embed/XVDDEsmbjuE?ecver=1",frameborder="0",allow="autoplay; encrypted-media",allowfullscreen)
